{
  "schema_version": 1,
  "quilt_loader": {
    "group": "org.quiltmc.qkl",
    "id": "qkl",
    "version": "${version}",
    "metadata": {
      "name": "Quilt Kotlin Libraries",
      "description": "Quilt's official Kotlin libraries",
      "contributors": {
        "QuiltMC: Quilt Kotlin Team": "Owner"
      },
      "contact": {
        "homepage": "https://quiltmc.org",
        "issues": "https://github.com/quiltmc/quilt-kotlin-libaries/issues",
        "sources": "https://github.com/quiltmc/quilt-kotlin-libraries"
      },
      "license": "Apache-2.0",
      "icon": "assets/qkl/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.0-beta.3"
      },
      {
        "id": "qkl_core",
        "versions": ">=${version}"
      },
      {
        "id": "qkl_qsl",
        "versions": ">=${version}"
      },
      {
        "id": "qkl_minecraft",
        "versions": ">=${version}"
      }
    ]
  },
  "modmenu": {
    "badges": [
      "library"
    ]
  }
}