{
  "schema_version": 1,
  "quilt_loader": {
    "group": "org.quiltmc.qkl",
    "id": "qkl_core",
    "version": "${version}",
    "metadata": {
      "name": "Quilt Kotlin Core",
      "description": "Core module for QKL",
      "contributors": {
        "QuiltMC: Quilt Kotlin Team": "Owner"
      },
      "contact": {
        "homepage": "https://quiltmc.org",
        "issues": "https://github.com/quiltmc/quilt-kotlin-libaries/issues",
        "sources": "https://github.com/quiltmc/quilt-kotlin-libraries"
      },
      "license": "Apache-2.0",
      "icon": "assets/qkl_core/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.0"
      }
    ],
    "provides": [
      {
        "id": "fabric-language-kotlin",
        "version": "1.8.0"
      }
    ],
    "language_adapters": {
      "kotlin": "org.quiltmc.qkl.core.adapter.KotlinAdapter"
    }
  },
  "modmenu": {
    "badges": [
      "library"
    ],
    "parent": {
      "id": "qkl",
      "name": "Quilt Kotlin Libraries",
      "description": "Official Kotlin libraries for modding in Kotlin",
      "icon": "assets/qkl_core/icon.png",
      "badges": [
        "library"
      ]
    }
  }
}